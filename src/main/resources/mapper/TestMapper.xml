<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.batch.repository.TestMapper">

    <select id="findTest" parameterType="map" resultType="com.example.batch.dto.TestDto">
        select
            test as test,
            content  as content
        from test
        where 1=1
        and test = #{str}
    </select>

</mapper>